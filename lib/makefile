all:
	$(MAKE) -C ./ihmbackground -f makefile all
	$(MAKE) -C ./ihmcapture -f makefile all
	$(MAKE) -C ./ihmfootprint -f makefile all
	$(MAKE) -C ./ihmperspective -f makefile all
